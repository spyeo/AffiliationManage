<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">


<tiles-definitions>
	<definition name="layouts-tiles" template="/WEB-INF/views/tiles/layouts.jsp">
		<put-attribute name="css" value="/WEB-INF/views/tiles/css.jsp" />
		<put-attribute name="js" value="/WEB-INF/views/tiles/js.jsp" />
		<put-attribute name="header" value="/WEB-INF/views/tiles/header.jsp" />
		<put-attribute name="content" value="" />
	</definition>
	
	<definition name="main/*.tiles" extends="layouts-tiles">
		<put-attribute name="content" value="/WEB-INF/views/main/{1}.jsp" />
	</definition>
	
	<definition name="franchise/*.tiles" extends="layouts-tiles">
		<put-attribute name="content" value="/WEB-INF/views/franchise/{1}.jsp" />
	</definition>
	
	<definition name="consulting/*.tiles" extends="layouts-tiles">
		<put-attribute name="content" value="/WEB-INF/views/consulting/{1}.jsp" />
	</definition>
	
	<definition name="empty" template="/WEB-INF/views/tiles/empty.jsp">
		<put-attribute name="content" value=""></put-attribute>
	</definition>
	
	<definition name="htmlcode/*/*.tiles" extends="empty">
		<put-attribute name="content" value="/WEB-INF/views/htmlcode/{1}/{2}.jsp"></put-attribute>
	</definition>

</tiles-definitions>